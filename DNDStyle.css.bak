Body{
	background-color: #FAEBD7;
}
* {
    box-sizing: border-box;
}
h1{
	color:White;
}


.body_content{
	box-sizing: border-box;
	height: 1000px;
}
li a:hover {
    background-color: #111;
}
li.active{
    background-color: #FF0000;
}
.navbar{
		width: 100%;
	background-color: grey;
}

.navbarmenu{
	box-sizing: border-box;
    list-style-type: none;
    margin: 0px;
    padding: 0px 0px;
	margin: 0px 0px 16px 0%;
}
.navbarmenu ul{
box-sizing: border-box;
width: 50%;
padding: 0px;

margin: 0px 25%;
}
.navbarmenu li{
	display: inline;
list-style-type:none;
}
.navbarmenu li a{
   box-sizing:border-box;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
	color: white;
	text-align: center;
	padding: 11px 0px;
	text-decoration: none;

}
.navbarmenu a{
display: inline-block;
	width: 20%;
	padding: 0% 2.5%;
}

.Modulenavbarmenu{
	box-sizing: border-box;
    list-style-type: none;
    margin: 0px;
    padding: 0px 0px;
	margin: 0px 0px 0px 0%;
}
.Modulenavbarmenu ul{
box-sizing: border-box;
width: 99%;
padding: 0px;

margin: 1%;
}
.Modulenavbarmenu li{
width: 100%;
margin:  2% 0px;
list-style-type:none;
}
.Modulenavbarmenu li a{
   box-sizing:border-box;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
	display:block;
	color: white;
	text-align: center;
	font-size: 5px;
	text-decoration: none;

}


.News_contentborder{
	border-style: solid;
    border-color: #ff0000 ;
	min-height: 20%;
	max-height: 23.5%;
	margin: 5px;
}
.News_contentobject{
	vertical-align: top;
	display: inline-block;
	height: 100%;
	max-width:49%;
	margin: 2px;

}
.News_botlinkbox{
margin-left: 90%;
}
.News_Header{
	min-height: 5%;
}
.PH_ads{
	display: inline-block;
    position: relative;
	width: 10%;
	height: 100%;
	background-color: lightgrey;
	vertical-align: top;
}

.PH_main_content{
	display: inline-block;
    position: relative;
	width: 69%;
	height: 100%;
	background-color: grey;
	vertical-align: top;
}

.PH_modules_list{
	display: inline-block;
    position: relative;
	width: 20%;
	height: 100%;
	background-color: Black;
	vertical-align: top;
}
.sickdragonimg_ph{
	min-height: 200px;
}

